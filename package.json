{
    "dependencies": {
        "@angular/animations": "^18.2.7",
        "@angular/cdk": "^18.2.6",
        "@angular/common": "^18.2.7",
        "@angular/compiler": "^18.2.7",
        "@angular/core": "^18.2.7",
        "@angular/forms": "^18.2.7",
        "@angular/material": "^18.2.6",
        "@angular/platform-browser": "^18.2.7",
        "@angular/platform-browser-dynamic": "^18.2.7",
        "@angular/router": "^18.2.7",
        "ng-gallery": "^11.0.0",
        "tslib": "^2.7.0",
        "zone.js": "~0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.7",
        "@angular/cli": "^18.2.7",
        "@angular/compiler-cli": "^18.2.7",
        "@types/jasmine": "~5.1.4",
        "angular-cli-ghpages": "^2.0.1",
        "eslint-config-angular": "github:jano-kucera/eslint-config-angular",
        "jasmine-core": "~5.3.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "~5.5.2"
    },
    "eslintConfig": {
        "extends": "eslint-config-angular",
        "ignorePatterns": [
            "node_modules/",
            "dist/",
            "public/",
            "package-lock.json"
        ]
    },
    "name": "pojazdny-pneuservis",
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "^4.24.0"
    },
    "prettier": {
        "jsonRecursiveSort": true,
        "multilineArraysWrapThreshold": 3,
        "plugins": [
            "prettier-plugin-css-order",
            "prettier-plugin-sort-json",
            "prettier-plugin-multiline-arrays"
        ]
    },
    "private": true,
    "scripts": {
        "build": "ng build",
        "deploy": "ng deploy",
        "lint": "npx eslint \"**/*.{ts,html}\" --fix && prettier --write \"**/*.{json,scss}\" --log-level silent",
        "ng": "ng",
        "start": "ng serve --open",
        "update-eslint": "npm install jano-kucera/eslint-config-angular",
        "update-packages": "npx npm-check-updates --interactive --format group",
        "watch": "ng build --watch --configuration development"
    },
    "version": "0.0.0"
}
